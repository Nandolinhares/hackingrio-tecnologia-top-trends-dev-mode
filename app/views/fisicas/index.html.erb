<p id="notice"><%= notice %></p>

<h1>Fisicas</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Telefone</th>
      <th>Endereco</th>
      <th>Token</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fisicas.each do |fisica| %>
      <tr>
        <td><%= fisica.nome %></td>
        <td><%= fisica.cpf %></td>
        <td><%= fisica.telefone %></td>
        <td><%= fisica.endereco %></td>
        <td><%= fisica.token %></td>
        <td><%= link_to 'Show', fisica %></td>
        <td><%= link_to 'Edit', edit_fisica_path(fisica) %></td>
        <td><%= link_to 'Destroy', fisica, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fisica', new_fisica_path %>
